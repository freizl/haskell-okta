{
  "name": "test-e2e",
  "version": "1.0.0",
  "description": "Okta OAuth Test Suite",
  "main": "index.js",
  "scripts": {
    "okta-hosted-login-server": "cd ../okta-hosted-login && cabal v2-exec okta-hosted-login-exe",
    "test:okta-hosted-login": "PORT=9191 protractor ./node_modules/okta-oidc-tck/e2e-tests/okta-hosted-login/conf.js",
    "custom-login-server": "cd ../custom-login && cabal v2-exec custom-login-exe",
    "test:custom-login": "PORT=9191 protractor ./node_modules/okta-oidc-tck/e2e-tests/custom-login/conf.js",
    "resource-server": "PORT=9191 cd ../resource-server && cabal v2-exec resource-server-exe",
    "test:resource-server": "jasmine ./node_modules/okta-oidc-tck/resource-server/specs/okta-resource-server-spec.js",
    "test": "npm run test:okta-hosted-login && npm run test:custom-login",
    "pretest": "webdriver-manager update --gecko false"
  },
  "author": "Haisheng Wu",
  "license": "Apache-2.0",
  "devDependencies": {
    "dotenv": "^8.1.0",
    "find-process": "^1.1.0",
    "forever-monitor": "^1.7.1",
    "jasmine": "^3.1.0",
    "jasmine-reporters": "^2.3.0",
    "okta-oidc-tck": "okta/okta-oidc-tck",
    "platform": "^1.3.5",
    "protractor": "^5.3.1",
    "wait-on": "^3.3.0"
  }
}
